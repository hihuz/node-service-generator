{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "lib": [
            "es2020",
        ],
        "sourceMap": true,
        "outDir": "./build",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "noImplicitAny": true,
        "noFallthroughCasesInSwitch": true,
        "noUnusedLocals": true,
        "noUnusedParameters": false,
        "declaration": true,
        "baseUrl": "."
    },
    "compileOnSave": false
}
